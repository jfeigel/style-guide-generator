{
  "name": "style-guide-generator",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/jfeigel/style-guide-generator",
    "type": "git"
  },
  "author": {
    "name": "James Feigel (@jfeigel)",
    "email": "james.feigel@gmail.com"
  },
  "license": "gpl-3.0",
  "scripts": {
    "start": "concurrently --kill-others -p [{name}] -n \"KOA,REACT\" -c \"black.bgWhite.bold,black.bgCyan.bold\" \"yarn run server\" \"yarn run web\"",
    "server": "./node_modules/.bin/nodemon server/index.js",
    "web": "react-scripts start",
    "mongo": "screen -dmS sgg-mongodb bash -c 'mongod --dbpath=/Users/jfeigel/Projects/style-guide-generator/data'",
    "redis": "screen -dmS sgg-redis redis-server",
    "services": "yarn run mongo && yarn run redis",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docs": "rm -rf docs && jsdoc -c jsdoc.conf.json",
    "docs:server": "(cd docs && python -m http.server 1337)",
    "docs:all": "yarn run docs && yarn run docs:server",
    "lint": "./node_modules/.bin/eslint ."
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/styles": "^4.9.6",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.0",
    "deepmerge": "^4.2.2",
    "history": "^4.10.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-generic-session": "^2.0.4",
    "koa-passport": "^4.1.3",
    "koa-redis": "^4.0.1",
    "koa-router": "^8.0.8",
    "koa-session": "^5.13.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.4",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "redis": "^3.0.2",
    "redis-commands": "^1.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-source-map-support": "^2.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "better-docs": "^1.4.7",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react-scripts": "3.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
