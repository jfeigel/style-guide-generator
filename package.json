{
  "name": "style-guide-generator",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/jfeigel/style-guide-generator",
    "type": "git"
  },
  "author": {
    "name": "James Feigel (@jfeigel)",
    "email": "james.feigel@gmail.com"
  },
  "license": "gpl-3.0",
  "scripts": {
    "start": "concurrently --kill-others -p [{name}] -n \"KOA,REACT\" -c \"black.bgWhite.bold,black.bgCyan.bold\" \"yarn run be\" \"yarn run fe\"",
    "be": "./node_modules/.bin/nodemon --watch server -e js server/index.js",
    "fe": "react-scripts start",
    "mongo": "screen -dmS mongodb bash -c 'mongod --dbpath=/data'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.0",
    "history": "^4.10.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-generic-session": "^2.0.4",
    "koa-passport": "^4.1.3",
    "koa-redis": "^4.0.1",
    "koa-router": "^8.0.8",
    "koa-session": "^5.13.1",
    "koa-static": "^5.0.0",
    "mongodb": "^3.5.5",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "redis": "^3.0.2",
    "redis-commands": "^1.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react-scripts": "3.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
